{
  "name": "hyperapp-example",
  "version": "1.0.0",
  "description": "hyperapp example",
  "main": "index.js",
  "scripts": {
    "test": "bablyfill test.js",
    "clear": "rimraf ./dist",
    "watch": "run-s build && opn dist/index.html && fs-onchange 'src/**' npm run build:js",
    "copy": "cp src/style.css dist && cp src/index.html dist",
    "build": "es6 t src/index.js lib/index.js && npm run bundle",
    "build:js": "es6 t src/index.js lib/index.js && browserify lib/index.js > dist/bundle.js",
    "bundle": "npm run clear && mkdirp dist && browserify lib/index.js > dist/bundle.js && npm run copy",
    "deploy": "npm run build && gh-pages -d dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soenkekluth/hyperapp-example.git"
  },
  "keywords": [],
  "author": "SÃ¶nke Kluth <soenke.kluth@gmail.com> (http://soenkekluth.com/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/soenkekluth/hyperapp-example/issues"
  },
  "homepage": "https://github.com/soenkekluth/hyperapp-example#readme",
  "dependencies": {
    "hyperapp": "^1.0.0"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "bablyfill": "^1.8.0",
    "browserify": "^14.5.0",
    "fs-onchange": "^1.0.0",
    "gh-pages": "^1.1.0",
    "jsdom": "^11.5.1",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.2",
    "opn": "^5.1.0",
    "opn-cli": "^3.1.0",
    "rimraf": "^2.6.2"
  }
}
